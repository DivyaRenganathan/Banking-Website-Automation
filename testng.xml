<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="utilites.Reporting" />
	</listeners>
	<!-- Test -->
	<test name="MicrosoftEdgeTest">
		<parameter name="browser" value="edge"></parameter>
		<parameter name="accounttype" value="CHECKING"></parameter>
		<classes>
			<class name="testCases.TC01_LoginTest" />
			<class name="testCases.TC02_LoginCredentialsTest" />
			<class name="testCases.TC03_RegisterTest" />
			<class name="testCases.TC04_OpenAccountTest" />
			<class name="testCases.TC05_AccountOverviewTest" />
			<class name="testCases.TC06_AccountDetailsTest" />
		</classes>
	</test>
	<!-- Test -->
	<test name="ChromeTest">
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="accounttype" value="CHECKING"></parameter>
		<classes>
			<class name="testCases.TC01_LoginTest" />
			<class name="testCases.TC02_LoginCredentialsTest" />
			<class name="testCases.TC03_RegisterTest" />
			<class name="testCases.TC04_OpenAccountTest" />
			<class name="testCases.TC05_AccountOverviewTest" />
			<class name="testCases.TC06_AccountDetailsTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
